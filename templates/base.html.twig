<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
																																														        and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			<link href="https://bootswatch.com/5/cosmo/bootstrap.min.css" rel="stylesheet"/>
			<script src="https://kit.fontawesome.com/4d71306872.js" crossorigin="anonymous"></script>


		{% endblock %}

		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
		{% endblock %}
	</head>
	<body>
		<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
			<div class="container-fluid">


				<div class="collapse navbar-collapse" id="navbarColor02">
					<ul class="navbar-nav me-auto">

						{% if app.user %}

							<li class="nav-item">
								<a class="nav-link active" href="#">Home
									<span class="visually-hidden">(current)</span>
								</a>
							</li>
							<li class="nav-item"></li>


							<a class="btn btn-primary" href="{{path('app_logout')}}">
								LogOut
							</a>
						{% endif %}


					</li>
					<li>
						{% set currentPath = path(app.request.attributes.get('_route'),
                       app.request.attributes.get('_route_params')) %}

						{{currentPath}}
					</li>


				</ul>

			</div>
		</div>
	</body>
</html></body></html></nav>{% block body %}{% endblock %}</body></html>
