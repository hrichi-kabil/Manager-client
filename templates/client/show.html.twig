{% extends 'base.html.twig' %}

{% block title %}Client
{% endblock %}

{% block body %}
	<h1>Client</h1>


	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ client.id }}</td>
			</tr>
			<tr>
				<th>NameClient</th>
				<td>{{ client.nameClient }}</td>
			</tr>
			<tr>
				<th>PhoneClient</th>
				<td>{{ client.phoneClient }}</td>
			</tr>
			<tr>
				<th>MailClient</th>
				<td>{{ client.mailClient }}</td>
			</tr>
		</tbody>
	</table>


	<a class="btn btn-success" href="{{ path('client_edit', {'id': client.id}) }}">
		<i class="fas fa-edit"></i>
	</a>

	<br>
	<br>

	{{ include('client/_delete_form.html.twig') }}
{% endblock %}
